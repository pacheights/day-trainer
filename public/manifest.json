{
  "name": "Exercise Break",
  "version": "1.0",
  "description": "Take a few minutes, a few times a day, to exercise",
  "manifest_version": 2,
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["./main.css"]
    }
  ]
}
