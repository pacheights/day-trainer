{
  "name": "Day Trainer",
  "version": "1.0",
  "description": "Take a few minutes, a few times a day, to exercise",
  "manifest_version": 2,
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "./popup/popup.html",
    "default_title": "Day Trainer"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
